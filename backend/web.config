<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="ReverseProxyInboundRule1" stopProcessing="true">
                    <match url="(.*)" />
                         <!-- Pordction URL PORT -->
                        <!-- 1101 is for the production and 1100 for the dev -->
                    <action type="Rewrite" url="http://localhost:1100/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
